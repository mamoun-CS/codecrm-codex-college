# Backend API Configuration
BACKEND_URL=https://your-api-domain.com

# Frontend Configuration
NEXT_PUBLIC_FRONTEND_URL=https://yourdomain.com
NEXT_PUBLIC_API_URL=https://your-api-domain.com
NEXT_PUBLIC_WEBSOCKET_URL=https://your-api-domain.com
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# Security Tokens
FRONTEND_WEBHOOK_TOKEN=your-webhook-secret
BACKEND_API_TOKEN=your-api-key
NEXT_PUBLIC_WEBHOOK_TOKEN=your-webhook-secret

# Facebook OAuth Integration
# Get your Facebook App ID from: https://developers.facebook.com/apps/
NEXT_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id_here

# TikTok OAuth Integration
# Get your TikTok Client Key from: https://developers.tiktok.com
NEXT_PUBLIC_TIKTOK_CLIENT_KEY=your_tiktok_client_key_here
